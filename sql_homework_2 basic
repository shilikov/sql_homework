create table genre (genre_ID serial primary key, genre_name varchar(40) not null); 
CREATE TABLE

create table musician_performer (artist_id serial primary key, artist_name varchar(40), alas varchar(40)); 
CREATE TABLE

create table album (album_id serial primary key, album_name varchar(40) not null, year_of_release integer, artist_id integer references musician_performer(artist_id));
CREATE TABLE

create table track (track_id serial primary key, album_id integer references album(album_id), track_name varchar(40), track_duration integer, genre integer references genre(genre_id));
CREATE TABLE

create table collections (collections_id serial primary key, track integer references track(track_id), artist_id integer references musician_performer(artist_id), collections_name varchar(40) not null, year_of_release integer);
CREATE TABLE

create table album_performer (album_id integer references album(album_id), album_name varchar(40), musician_performer integer references musician_performer(artist_id));
CREATE TABLE

create table song_performer (track_id integer references track(track_id),track_name varchar(40) not null, musician_performer integer references musician_performer(artist_id
));
CREATE TABLE

alter table album drop column artist_id;
ALTER TABLE

alter table album_performer add column album_performer serial primary key;
ALTER TABLE

alter table album add column album_performer integer references album_performer(album_performer);
ALTER TABLE

alter table song_performer add column song_performer serial primary key;
ALTER TABLE

alter table track add column song_performer integer references song_performer(song_performer);
